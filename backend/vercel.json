{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/**"],
        "excludeFiles": [
          "node_modules/@types/**",
          "node_modules/typescript/**",
          "node_modules/onnxruntime-node/**",
          "node_modules/ipfs-http-client/**",
          "node_modules/@ipld/**",
          "node_modules/ipfs-core-types/**",
          "node_modules/ipfs-core-utils/**",
          "node_modules/arweave/node_modules/**",
          "**/*.test.ts",
          "**/*.spec.ts",
          "**/*.map",
          "**/*.d.ts.map",
          "models/**",
          "test-integration.js",
          "start-and-test.sh"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "src/index.ts": {
      "maxDuration": 30,
      "memory": 1024
    }
  }
}

{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.ts": {
      "maxDuration": 30,
      "excludeFiles": "{node_modules/onnxruntime-node/**,node_modules/ipfs-http-client/**,node_modules/@ipld/**,node_modules/ipfs-core-types/**,node_modules/ipfs-core-utils/**,node_modules/ipfs-unixfs/**,node_modules/libp2p/**,node_modules/@libp2p/**}"
    }
  }
}
